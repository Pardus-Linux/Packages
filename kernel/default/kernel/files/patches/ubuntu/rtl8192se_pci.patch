+#ifdef TO_DO_LIST
+static	bool	
+efuse_ParsingMap(char* szStr,u32* pu4bVal,u32* pu4bMove)
+{
+	char* 		szScan = szStr;
+
+	if(szStr == NULL || pu4bVal == NULL || pu4bMove == NULL)
+	{
+		return false;
+	}
+
+	*pu4bMove = 0;
+	*pu4bVal = 0;
+
+	while(	*szScan != '\0' && 
+			(*szScan == ' ' || *szScan == '\t') )
+	{
+		szScan++;
+		(*pu4bMove)++;
+	}
+
+	if(!IsHexDigit(*szScan))
+	{
+		return false;
+	}
+
+	do
+	{
+		(*pu4bVal) <<= 4;
+		*pu4bVal += MapCharToHexDigit(*szScan);
+
+		szScan++;
+		(*pu4bMove)++;
+	} while(IsHexDigit(*szScan));
+
+	return true;
+	
+}	/* efuse_ParsingMap */
