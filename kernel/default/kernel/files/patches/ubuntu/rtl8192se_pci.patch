+		}
+
+	}
+
+	writeVal = (rfa_pwr[3]<<24) | (rfa_pwr[2]<<16) |(rfa_pwr[1]<<8) |rfa_pwr[0];
+
+	rtl8192_setBBreg(dev, RegOffset[index], 0x7f7f7f7f, writeVal);
+	RT_TRACE(COMP_POWER,"Set 0x%x = %08x\n",RegOffset[index], writeVal);
+}
