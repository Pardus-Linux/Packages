+			priv->rtllib->SystemQueryDataRateCount++;
+			if(bMaxRateMcs15)
+				return 600;
+			else
+				return 270;
+		}
+
+		if (bMaxRateMcs15)
+			rate = rtl8192_decorate_txrate_by_singalstrength(Sgstrength, (u8*)SS_Rate_Map_N_MCS15,
+					sizeof(SS_Rate_Map_N_MCS15)/2);
