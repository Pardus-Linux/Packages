+	struct rtllib_device* ieee = priv->rtllib;
+	struct sk_buff* skb = rtllib_get_beacon(ieee);
+
+	if (unlikely(!skb)){
+		printk("========>error alloc skb\n");
+		return;
+	}
+
+	priv->rtllib->softmac_data_hard_start_xmit(skb, dev, ieee->rate);
+}
