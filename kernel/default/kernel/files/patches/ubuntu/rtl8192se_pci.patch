+		write_cam(dev, (4*6)+5, read_cam(dev, (key_idx*6)+5) );
+	}
+#endif
+#ifdef _RTL8192_EXT_PATCH_
+	priv->rtllib->wx_set_enc = 0;
+	printk("===================>%s():set ieee->wx_set_enc 0\n",__FUNCTION__);
+end_hw_sec:
+#else
+end_hw_sec:
+	priv->rtllib->wx_set_enc = 0;
