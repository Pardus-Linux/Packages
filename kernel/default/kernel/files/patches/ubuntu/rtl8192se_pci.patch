+						ieee->pairwise_key_type,
+						zero_addr[key_idx],
+						0,
+						hwkey);
+				set_swcam( dev,
+						key_idx,
+						key_idx,
+						ieee->pairwise_key_type,
+						zero_addr[key_idx],
+						0,
+						hwkey,
+						0);
+			}
+			else{
+			setKey( dev,
+					31,
+					key_idx,
+						ieee->pairwise_key_type,
+					ieee->ap_mac_addr,
+					0,
+					hwkey);
+			set_swcam( dev,
+					31,
+					key_idx,
+						ieee->pairwise_key_type,
+					ieee->ap_mac_addr,
+					0,
+					hwkey,
+					0);
+		}
+	}
+#endif
+	}
+
+#if 0
+	if(wrqu->encoding.length==0 && (wrqu->encoding.flags >>8) == 0x8 ){
