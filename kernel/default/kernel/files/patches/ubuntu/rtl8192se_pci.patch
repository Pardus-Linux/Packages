+				}
+
+				if (pFwHdr->IMG_SRAM_SIZE > sizeof(pFirmware->FwEMEM))
+				{
+					RT_TRACE(COMP_ERR, "%s: memory for data image is less than EMEM required\n",\
+							__FUNCTION__);	
+					goto DownloadFirmware_Fail;
+				}
+				else
+				{	
+					pucMappedFile += pFirmware->FwIMEMLen;
