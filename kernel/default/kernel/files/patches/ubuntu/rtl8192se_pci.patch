+
+			WAPI_TRACE(WAPI_API, "%s(): WAPI_CMD_SET_KEY!\n",__FUNCTION__);
+			if (!ieee->WapiSupport){
+				ret = -1;
+				break;
+			}
+			if(wrq->u.data.length < 26){
+				ret = -1;
+				break;
+			}
+
+			copy_from_user(data, (u8 *)wrq->u.data.pointer, wrq->u.data.length);
