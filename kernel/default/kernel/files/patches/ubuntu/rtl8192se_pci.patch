+	} else if (priv->rtllib->mode == WIRELESS_MODE_N_5G) {
+		return 580;
+	} else {
+		return 2;
+	}
+
+	if (priv->rtllib->GetHalfNmodeSupportByAPsHandler(dev)) {
+		if (rate < 0x80)
+			return rate;
+		else
+			return HTHalfMcsToDataRate(priv->rtllib, rate);
+	} else {
+                return CONVERT_RATE(priv->rtllib, rate);
