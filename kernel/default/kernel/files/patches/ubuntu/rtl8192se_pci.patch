+u8 rtl8192_decorate_txrate_by_singalstrength(u32 SignalStrength, u8 *SS_Rate_Map, u8 MapSize)
+{
+	u8 index = 0;
+
+	for (index = 0; index < (MapSize * 2); index += 2) {
+		if (SignalStrength > SS_Rate_Map[index])
+			return SS_Rate_Map[index+1];
+	}
+
+	return MGN_1M;
+}
