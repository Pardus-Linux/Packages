+			bool					bUpdate;
+			bool					bAuthenticator;
+			u8					PeerAddr[6];
+			u8					WapiAEPNInitialValueSrc[16] = {0x37,0x5C,0x36,0x5C,0x36,0x5C,0x36,0x5C,0x36,0x5C,0x36,0x5C,0x36,0x5C,0x36,0x5C} ;
