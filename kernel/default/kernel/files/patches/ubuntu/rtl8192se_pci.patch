+static u8 my_atoi(const char *arg)
+{
+	u8 val = 0;
+	for(; ; arg++){
+		switch (*arg){
+			case '0'...'9':
+				val = 10*val + (*arg-'0');
+				break;
+			default:
+				return val;
+		}
+	}
+	return val;
+}
