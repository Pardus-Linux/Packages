+	del_timer_sync(&ieee->crypt_deinit_timer);
+	rtllib_crypt_deinit_entries(ieee, 1);
+
+#ifdef _RTL8192_EXT_PATCH_
+	for (j=0;j<MAX_MP; j++)
+	{
+		if (ieee->cryptlist[j] == NULL)
+			continue;
+		for (i = 0; i < WEP_KEYS; i++) {
+			crypt = ieee->cryptlist[j]->crypt[i];
+
+			if (crypt)
