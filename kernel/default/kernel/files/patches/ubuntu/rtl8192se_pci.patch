+long
+rtl8192se_signal_scale_mapping(struct r8192_priv * priv,
+	long currsig
+	)
+{
+	long retsig = 0;
+
+	if(currsig> 47)
+		retsig = 100;
+	else if(currsig >14 && currsig <=47)
+		retsig = 100 - ((47-currsig)*3)/2;
+	else if(currsig >2 && currsig <=14)
+		retsig = 48-((14-currsig)*15)/7;
+	else if(currsig >=0)
+		retsig = currsig * 9+1;
+
+	return retsig;
+}
