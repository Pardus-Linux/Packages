+u8 GetFreeRATRIndex8192SE (struct net_device* dev)
+{
+	struct r8192_priv* priv = rtllib_priv(dev);
+	u8 bitmap = priv->RATRTableBitmap;
+	u8 ratr_index = 0;
+	for(	;ratr_index<7; ratr_index++)
+	{
+		if((bitmap & BIT0) == 0)
+			{
+				priv->RATRTableBitmap |= BIT0<<ratr_index;
+				return ratr_index;
+			}
+		bitmap = bitmap >>1;
+	}
+	return ratr_index;
+}
