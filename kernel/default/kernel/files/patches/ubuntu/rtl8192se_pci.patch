+void rtllib_softmac_stop_scan(struct rtllib_device *ieee)
+{
+
+
+	down(&ieee->scan_sem);
+	ieee->scan_watch_dog = 0;
+	if (ieee->scanning_continue == 1){
+		ieee->scanning_continue = 0;
+		ieee->actscanning = 0;
