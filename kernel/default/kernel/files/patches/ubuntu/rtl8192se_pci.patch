+					idx,
+					idx,
+					alg,
+					zero,
+					0,
+					key,
+					0);
+		}
+		else if (group)
+		{
+			ieee->group_key_type = alg;
+			setKey( dev,
+					idx,
+					idx,
+					alg,
+					broadcast_addr,
+					0,
+					key);
+			set_swcam( dev,
+					idx,
+					idx,
+					alg,
+					broadcast_addr,
+					0,
+					key,
+					0);
+		}
+		else
+		{
+			#ifdef RTL8192E
+			if ((ieee->pairwise_key_type == KEY_TYPE_CCMP) && ieee->pHTInfo->bCurrentHTSupport){
+							write_nic_byte(dev, 0x173, 1);
