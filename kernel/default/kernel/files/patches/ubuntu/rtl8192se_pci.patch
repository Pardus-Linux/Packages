+int _rtl8192_up(struct net_device *dev,bool is_silent_reset)
+{
+#ifdef _RTL8192_EXT_PATCH_
+	if(_rtl8192_mesh_up(dev, is_silent_reset) == -1)
+		return -1;
+#else
+	if(_rtl8192_sta_up(dev, is_silent_reset) == -1)
+		return -1;
+#endif
+
+	return 0;
+}
