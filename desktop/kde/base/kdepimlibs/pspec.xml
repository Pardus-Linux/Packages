        </BuildDependencies>
        <!-- Nobody uses tht patch now?
        <Patches>
             Fix for #13123 kres-migrator bug
            <Patch>kres-migrate-only-when-resources-present.diff</Patch>
        </Patches>
        -->
    </Source>
    <Package>
        <Name>kdepimlibs</Name>
        <RuntimeDependencies>
            <Dependency versionFrom="4.5.2">kdelibs</Dependency>
            <Dependency versionFrom="1.4.0">akonadi</Dependency>
            <Dependency versionFrom="4.7.0">qt</Dependency>
            <Dependency>boost</Dependency>
            <Dependency>gpgme</Dependency>
            <Dependency>cyrus-sasl</Dependency>
            <Dependency>openldap-client</Dependency>
            <Dependency>libical</Dependency>
            <Dependency>pth</Dependency>
            <Dependency versionFrom="4.4.2">phonon</Dependency>
            <Dependency>libgpg-error</Dependency>
        </RuntimeDependencies>
        <Conflicts>
            <!-- /usr/kde/4/share/apps/libkholidays/holiday_XX files conflict with old kdepim -->
            <Package versionTo="4.2.4">kdepim</Package>
        </Conflicts>
        <Files>
            <Path fileType="library">/usr/lib</Path>
            <Path fileType="data">/usr/share</Path>
            <Path fileType="doc">/usr/share/doc</Path>
        </Files>
        <AdditionalFiles>
            <AdditionalFile target="/usr/share/kde4/apps/libkholidays/plan2/holiday_tr_tr" permission="0644" owner="root">holiday_tr_tr</AdditionalFile>
        </AdditionalFiles>
    </Package>

    <Package>
        <Name>kdepimlibs-devel</Name>
        <Summary>Development package for KDE4 PIM libraries</Summary>
        <Description>Contains development tools and header files for KDE4 PIM (Personal Information Management) base libraries.</Description>
        <RuntimeDependencies>
            <Dependency versionFrom="4.5.2">kdepimlibs</Dependency>
            <Dependency versionFrom="4.5.2">kdelibs-devel</Dependency>
            <Dependency versionFrom="4.7.0">qt-devel</Dependency>
            <Dependency>boost-devel</Dependency>
        </RuntimeDependencies>
        <Files>
            <Path fileType="header">/usr/include</Path>
            <Path fileType="data">/usr/lib/gpgmepp</Path>
            <Path fileType="data">/usr/lib/cmake</Path>
            <Path fileType="data">/usr/share/kde4/apps/cmake</Path>
        </Files>
    </Package>
