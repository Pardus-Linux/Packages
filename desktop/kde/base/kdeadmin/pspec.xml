        <License>GPLv2</License>
        <IsA>app:gui</IsA>
        <Summary>KDE4 Administration Applications</Summary>
        <Description>kdeadmin contains administration related applications of KDE4. In this package, kcron (Task Scheduler module in System Settings), ksystemlog (system log viewer) and system-config-printer-kde (printer configuration application) applications are included.</Description>
        <Archive type="tarbz2" sha1sum="f5e64abed05133e99f37779e1eaf15c4d79d9c09">mirrors://kde/stable/4.5.4/src/kdeadmin-4.5.4.tar.bz2</Archive>
        <BuildDependencies>
            <Dependency versionFrom="4.5.4">kdelibs-devel</Dependency>
            <Dependency>python-cups</Dependency>
            <Dependency>system-config-printer</Dependency>
            <Dependency versionFrom="4.7.0">qt-devel</Dependency>
        </BuildDependencies>
        <Patches>
            <Patch level="1">do-not-compile-kuser.diff</Patch>
            <Patch level="1">kdeadmin-4.2.85-printing.patch</Patch>
            <Patch level="1">fix-unexpected-type-int-in-scpk.diff</Patch>
        </Patches>
    </Source>
    <Package>
        <Name>kdeadmin</Name>
        <RuntimeDependencies>
            <!-- needed by printer configuration GUI system-config-printer-kde -->
            <Dependency versionFrom="4.5.4">kdelibs</Dependency>
            <Dependency versionFrom="4.7.0">qt</Dependency>
            <Dependency>system-config-printer</Dependency>
            <Dependency>python-cups</Dependency>
        </RuntimeDependencies>
        <Replaces>
            <Package>kdeadmin4</Package>
            <Package>ksystemlog</Package>
        </Replaces>
        <Files>
            <Path fileType="executable">/usr/bin</Path>
            <Path fileType="library">/usr/lib</Path>
            <Path fileType="data">/usr/share</Path>
        </Files>
    </Package>
